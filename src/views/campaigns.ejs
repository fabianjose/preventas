
 <%- include ('partials/header')  %>


<div class="container mt-5">
	<h2>Nueva Campaña</h2>
	<form class="form row">
		<div class="form-group">
			<label>Descripción</label>
			<input class="form-control" type="text" name="">
		</div>
	</form>
	<hr>
	<table class="table">
		<thead>
			<tr>
				<th>ID</th><th>Descripcion</th>
			</tr>
		</thead>
		<tbody>
			<% cams.forEach(c => {%>
			<tr>
				<td><%= c.id %></td><td><%= c.descripcion %></td>
			</tr>
			<%})%>
			
		</tbody>
		
	</table>
		
</div>